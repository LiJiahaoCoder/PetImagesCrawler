{
    "index": "ReadMe.md",
    "source": "./source",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": {
                    "access": ["public", "protected"],
                    "autoPrivate": false
                },
                "test": {
                    "source": "./test",
                    "interfaces": ["describe", "it"],
                    "includes": ["\\.js$"]
                }
            }
        }
    ]
}
